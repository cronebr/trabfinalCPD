class Pokemon{
  private: string name;
  private: int pokedex_number;
  private: string type1;
  private: string type2;
  private: int hp_stat;
  private: int attack_stat;
  private: int defense_stat;
  private: int special_attack_stat;
  private: int special_defense_stat;
  private: int speed_stat;
  
  string get_name();
    return name;
  }
  
  int get_pokedex_number();
    return pokedex_number;
  }
  
  string get_type1();
    return type1;
  }
  
  string get_type2();
    return type2;
  }
  
  int get_hp_stat();
    return hp_stat;
  }
  
  int get_attack_stat();
    return attack_stat;
  }
  
  int get_defense_stat();
    return defense_stat;
  }
  
  int get_special_attack_stat();
    return special_attack_stat;
  }
  
  int get_special_defense_stat();
    return special_defense_stat;
  }
  
  int get_speed_stat();
    return speed_stat;
  }
