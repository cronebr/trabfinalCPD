class Pokemon{
  private: string name;
  private: int pokedex_number;
  private: string type1;
  private: string type2;
  private: int hp_stat;
  private: int attack_stat;
  private: int defense_stat;
  private: int special_attack_stat;
  private: int special_defense_stat;
  private: int speed_stat;
  
  Pokemon::Pokemon(){
    this->name = 0;
    this->pokedex_number = 0;
    this->type1 = 0;
    this->type2 = 0;
    this->hp_stat = 0;
    this->attack_stat = 0;
    this->defense_stat = 0;
    this->special_attack_stat = 0;
    this->special_defense_stat = 0;
    this->speed_stat = 0;
  
  string get_name(){
    return this->name;
  }
  
  int get_pokedex_number(){
    return this->pokedex_number;
  }
  
  string get_type1(){
    return this->type1;
  }
  
  string get_type2(){
    return this->type2;
  }
  
  int get_hp_stat(){
    return this->hp_stat;
  }
  
  int get_attack_stat(){
    return this->attack_stat;
  }
  
  int get_defense_stat(){
    return this->defense_stat;
  }
  
  int get_special_attack_stat(){
    return this->special_attack_stat;
  }
  
  int get_special_defense_stat(){
    return this->special_defense_stat;
  }
  
  int get_speed_stat(){
    return this->speed_stat;
  }
  
  void insert_name(insert){
    this->name = insert;
  }
  
  void insert_pokedex_number(insert){
    this->pokedex_number = insert;
  }
  
  void insert_type1(insert){
    this->type1 = insert;
  }
  
  void insert_type2(insert){
    this->type2 = insert;
  }
  
  void insert_hp_stat(insert){
    this->hp_stat = insert;
  }
  
  void insert_attack_stat(insert){
    this->attack_stat = insert;
  }
  
  void insert_defense_stat(insert){
    this->defense_stat = insert;
  }
  
  void insert_special_attack_stat(insert){
    this->special_attack_stat = insert;
  }
  
  void insert_special_defense_stat(insert){
    this->special_defense_stat = insert;
  }
  
  void insert_speed_stat(){
    this->speed_stat = insert;
